<Application x:Class="Egate_Payroll.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Egate_Payroll"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FontIcons.FontAwesome;component/fontawesome.xaml"/>
                <ResourceDictionary Source="/Styles/converters.xaml"/>
                <ResourceDictionary Source="/Styles/main styles.xaml"/>
                <ResourceDictionary Source="/Styles/bootstrap buttons.xaml"/>
                <ResourceDictionary Source="/Styles/datagrid styles.xaml"/>
                <ResourceDictionary Source="/Styles/monthly calendar day button.xaml"/>
                <ResourceDictionary Source="/Styles/tax calendar day button.xaml"/>
                <ResourceDictionary Source="/Styles/tax calendar.xaml"/>
                <ResourceDictionary Source="/Styles/custom tab item style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
